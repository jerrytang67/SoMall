{
  "name": "ng-alain",
  "version": "8.9.0",
  "description": "ng-zorro-antd admin panel front-end framework",
  "author": "tt-tjw@qq.com",
  "license": "MIT",
  "keywords": [
    "antd",
    "angular",
    "component"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 192.168.3.50",
    "hmr": "ng serve -c=hmr",
    "build": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --aot=false --build-optimizer=false --base-href /admin/",
    "analyze": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p tsconfig.app.json -c tslint.json \"src/**/*.ts\" --fix",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "node scripts/color-less.js",
    "icon": "ng g ng-alain:plugin icon",
    "api": "node ./swagger/codegen.v3.js",
    "new": "plop"
  },
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/cdk": "^11.2.6",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "@aspnet/signalr": "^1.0.26",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@ckeditor/ckeditor5-build-classic": "^27.0.0",
    "@ckeditor/ckeditor5-build-decoupled-document": "^27.0.0",
    "@datorama/akita": "^6.1.3",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "ajv": "^7.2.3",
    "angular2-baidu-map": "^8.1.1",
    "file-saver": "^2.0.5",
    "modernizr": "^3.9.1",
    "moment": "^2.27.0",
    "ng-chat": "^3.0.3",
    "ng-zorro-antd": "^11.3.0",
    "ngx-bootstrap": "^6.2.0",
    "ngx-toastr": "^13.2.1",
    "oidc-client": "^1.11.5",
    "qrious": "^4.0.2",
    "rxjs": "~6.5.5",
    "screenfull": "^5.1.0",
    "simple-line-icons": "^2.5.5",
    "source-sans-pro": "^3.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.5",
    "@angular/language-service": "~11.2.5",
    "@angularclass/hmr": "^3.0.0",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "@types/node": "^14.14.35",
    "akita-schematics": "^2.0.1",
    "codecov": "^3.8.1",
    "codelyzer": "^6.0.1",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "less-bundle-promise": "^1.0.7",
    "lint-staged": "^10.5.4",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~7.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "swagger-angular-codegen": "^0.1.8",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.3",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "npm run lint:ts",
        "git add"
      ],
      "src/**/*.less": [
        "npm run lint:style",
        "git add"
      ]
    },
    "ignore": [
      "src/assets/*"
    ]
  }
}